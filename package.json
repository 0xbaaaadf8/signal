{
  "name": "signal",
  "version": "0.0.1",
  "description": "A friendly music sequencer application for OS X and Windows.",
  "main": "src/node/main.js",
  "scripts": {
    "start": "webpack-dev-server --hot",
    "build": "react-scripts build && npm run copy-electron-files",
    "watch": "webpack --watch",
    "dev": "nf start",
    "test": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "electron": "cross-env-shell ELECTRON_START_URL=http://localhost:3000 \"node ./scripts/wait-server 3000 && electron .\"",
    "flow": "flow",
    "copy-electron-files": "cp electron-package.json build/package.json && cp src/main.js build/",
    "clean": "shx rm -rf build && shx rm -rf pkg",
    "package": "npm run clean && npm run build && electron-packager ./build signal --electron-version=1.7.9 --platform=win32,darwin --arch=x64 --out=./pkg --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryohey/signal.git"
  },
  "author": "ryohey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryohey/signal/issues"
  },
  "homepage": "./",
  "dependencies": {
    "color": "^3.0.0",
    "eventemitter3": "^3.1.0",
    "file-dialog": "0.0.7",
    "flow-bin": "^0.71.0",
    "lodash": "^4.17.10",
    "mdi": "^2.2.43",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-sizeme": "^2.4.3",
    "react-split-pane": "^0.1.77",
    "recompose": "^0.27.0",
    "resize-observer-polyfill": "^1.5.0",
    "serializr": "^1.2.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.3",
    "@storybook/react": "^3.4.3",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.108",
    "@types/recompose": "^0.26.1",
    "@types/webpack-env": "^1.13.6",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "custom-react-scripts": "^0.2.2",
    "electron": "^2.0.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^12.0.2",
    "foreman": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "mobx-react-devtools": "^5.0.1",
    "shx": "^0.2.2",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.5",
    "ts-loader": "^4.3.0",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
}
