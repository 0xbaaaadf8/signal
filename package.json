{
  "name": "signal",
  "version": "0.0.1",
  "description": "A friendly music sequencer application for OS X and Windows.",
  "main": "src/main.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run copy-electron-files",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "nf start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "electron": "electron .",
    "flow": "flow",
    "copy-electron-files": "cp electron-package.json build/package.json && cp src/main.js build/",
    "clean": "shx rm -rf build && shx rm -rf pkg",
    "package": "npm run clean && npm run build && electron-packager ./build signal --electron-version=1.7.9 --platform=win32,darwin --arch=x64 --out=./pkg --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryohey/signal.git"
  },
  "author": "ryohey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryohey/signal/issues"
  },
  "homepage": "./",
  "dependencies": {
    "color": "^3.0.0",
    "eventemitter3": "^3.0.1",
    "file-dialog": "0.0.7",
    "flow-bin": "^0.66.0",
    "lodash": "^4.11.1",
    "mdi": "^2.0.46",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-sizeme": "^2.3.6",
    "react-split-pane": "^0.1.66",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "resize-observer-polyfill": "^1.4.2",
    "serializr": "^1.1.14"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.12",
    "@storybook/react": "^3.3.0-alpha.2",
    "cross-env": "^5.1.1",
    "custom-react-scripts": "^0.2.0",
    "electron": "^1.7.8",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^11.0.1",
    "foreman": "^2.0.0",
    "mobx-react-devtools": "^4.2.15",
    "shx": "^0.2.2",
    "ts-loader": "^4.0.1",
    "typescript": "^2.7.2",
    "webpack": "^4.0.1"
  }
}
